config_version=4

_global_script_classes=[  ]
_global_script_class_icons={
}

[GodotXUnit]

target_assembly="Tests"

[MonoCustomResourceRegistry]

ClassPrefix=""
SearchType=0
ResourceScriptDirectories=[ "res://" ]

[application]

config/name="potions-game"
run/main_scene="res://src/Core/Game.tscn"
config/icon="res://assets/icon.png"

[autoload]

Logger="*res://src/Core/Utils/Logger.tscn"
SceneManager="*res://src/Core/Managers/SceneManager.tscn"
UiManager="*res://src/Core/Managers/UiManager.tscn"

[display]

window/size/test_width=1920
window/size/test_height=1080
window/stretch/mode="2d"
window/stretch/aspect="keep_height"

[editor_plugins]

enabled=PoolStringArray( "res://addons/GodotXUnit/plugin.cfg", "res://addons/MonoCustomResourceRegistry/plugin.cfg" )

